{
  "name": "@vision/core",
  "author": "Bilo Lwabona",
  "version": "0.1.3",
  "description": "A Typescript library for frontend and backend shared code of the Vision platform.",
  "main": "./dist/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "version": "yarn changelog",
    "dev": "yarn build:watch",
    "start": "webpack serve --open",
    "build:webpack": "webpack",
    "build:ts": "npx tsc --declarationMap && bash ./scripts/build-dist.sh",
    "build:watch": "npx tsc -w",
    "test": "yarn test:unit",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "test:clearCache": "jest --clearCache",
    "test:watch": "yarn test:unit --watchAll",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [],
  "license": "ISC",
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/addon-interactions": "^8.0.8",
    "@storybook/addon-links": "^8.0.8",
    "@storybook/addon-onboarding": "^8.0.8",
    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
    "@storybook/blocks": "^8.0.8",
    "@storybook/react": "^8.0.8",
    "@storybook/react-webpack5": "^8.0.8",
    "@storybook/test": "^8.0.8",
    "@types/jest": "^29.5.2",
    "@types/react": "^18.2.79",
    "auto-changelog": "^2.4.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.1",
    "file-loader": "^6.2.0",
    "jest": "^29.5.0",
    "sass-loader": "^14.2.1",
    "storybook": "^8.0.8",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.1.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "node-sass": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
